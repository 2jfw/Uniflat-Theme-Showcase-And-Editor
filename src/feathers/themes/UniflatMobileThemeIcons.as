		private static function getTextureForIcon(atlas : TextureAtlas,
		                                          name : String) : Texture
		{
			const texture : Texture = atlas.getTexture(name);
			if (!texture)
			{
				trace("[UniflatThemeIcons - Warning] Texture for icon '" + name + "' not found.");
			}
			return texture;
		}

